{
   "background": {
      "persistent": false,
      "scripts": [ "js/bg.1.js" ]
   },
   "browser_action": {
      "default_title": "Cookie Cutter"
   },
   "description": "Save, edit, and load cookies.",
   "manifest_version": 2,
   "minimum_chrome_version": "54.0",
   "name": "Cookie Cutter",
   "offline_enabled": true,
   "options_ui": {
      "open_in_tab": true,
      "page": "manager.html"
   },
   "permissions": [ "cookies", "\u003Call_urls>", "storage" ],
   "version": "0.2"
}
